{
  "expo": {
    "name": "VeilVPN",
    "slug": "veil-vpn",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./generated-icon.png",
    "userInterfaceStyle": "dark",
    "splash": {
      "image": "./generated-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#2c3e50"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.veilvpn.app"
    },
    "android": {
      "package": "com.veilvpn.app",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./generated-icon.png",
        "backgroundColor": "#2c3e50"
      },
      "permissions": [
        "INTERNET",
        "ACCESS_NETWORK_STATE",
        "FOREGROUND_SERVICE"
      ]
    },
    "web": {
      "favicon": "./generated-icon.png"
    },
    "extra": {
      "eas": {
        "projectId": "veil-vpn"
      }
    }
  },
  "name": "VeilVPN",
  "displayName": "Veil VPN"
}
